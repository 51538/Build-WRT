cd immortalwrt
rm -f feeds.conf.default
wget https://raw.githubusercontent.com/openwrt/openwrt/refs/heads/openwrt-24.10/feeds.conf.default
echo -e "\nsrc-git nss_packages https://github.com/qosmio/nss-packages.git" >> "feeds.conf.default"
echo "src-git sqm_scripts_nss https://github.com/qosmio/sqm-scripts-nss.git" >> "feeds.conf.default"  
